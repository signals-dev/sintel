{
  "version": 1,
  "projects": {
    "nx-react": {
      "root": "apps/nx-react",
      "sourceRoot": "apps/nx-react/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "outputPath": "dist/apps/nx-react",
            "index": "apps/nx-react/src/index.html",
            "main": "apps/nx-react/src/main.tsx",
            "polyfills": "apps/nx-react/src/polyfills.ts",
            "tsConfig": "apps/nx-react/tsconfig.app.json",
            "assets": [
              "apps/nx-react/src/favicon.ico",
              "apps/nx-react/src/assets"
            ],
            "styles": ["apps/nx-react/src/styles.scss"],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/nx-react/src/environments/environment.ts",
                  "with": "apps/nx-react/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "nx-react:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "nx-react:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/nx-react/.eslintrc",
            "tsConfig": [
              "apps/nx-react/tsconfig.app.json",
              "apps/nx-react/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/nx-react/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/nx-react/jest.config.js",
            "tsConfig": "apps/nx-react/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "nx-react-e2e": {
      "root": "apps/nx-react-e2e",
      "sourceRoot": "apps/nx-react-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/nx-react-e2e/cypress.json",
            "tsConfig": "apps/nx-react-e2e/tsconfig.e2e.json",
            "devServerTarget": "nx-react:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "nx-react:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/nx-react-e2e/.eslintrc",
            "tsConfig": ["apps/nx-react-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/nx-react-e2e/**"]
          }
        }
      }
    },
    "session": {
      "root": "libs/session",
      "sourceRoot": "libs/session/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "libs/session/.eslintrc",
            "tsConfig": [
              "libs/session/tsconfig.lib.json",
              "libs/session/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/session/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/session/jest.config.js",
            "tsConfig": "libs/session/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "dashboard": {
      "root": "libs/dashboard",
      "sourceRoot": "libs/dashboard/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "libs/dashboard/.eslintrc",
            "tsConfig": [
              "libs/dashboard/tsconfig.lib.json",
              "libs/dashboard/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/dashboard/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/dashboard/jest.config.js",
            "tsConfig": "libs/dashboard/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "common": {
      "root": "libs/common",
      "sourceRoot": "libs/common/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "libs/common/.eslintrc",
            "tsConfig": [
              "libs/common/tsconfig.lib.json",
              "libs/common/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/common/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/common/jest.config.js",
            "tsConfig": "libs/common/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "eslint"
      }
    },
    "@nrwl/react": {
      "application": {
        "style": "scss",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "scss"
      },
      "library": {
        "style": "scss",
        "linter": "eslint"
      }
    },
    "@nrwl/next": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/web": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/nx-plugin": {
      "plugin": {
        "linter": "eslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "nx-react"
}
